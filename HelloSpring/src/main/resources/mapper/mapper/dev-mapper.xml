<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="dev">
	<insert id="insertDemo" parameterType="dev">
		INSERT INTO DEV VALUES(seq_dev_no.nextval,#{devName},#{devAge},
		#{devEmail},#{devGender},
<!-- 		<foreach collection="devLang" item="a" open="'"
		separator="," close="'">
			${a}		
		</foreach>	 -->
		#{devLang, typeHandler=com.spring.hellospring.common.StringArrayTypeHandler}
		)
	</insert>
	
	<resultMap id="devMap" type="dev">
		<result column="devLang" property="devLang" typeHandler="com.spring.hellospring.common.StringArrayTypeHandler"/>
	</resultMap>
	<select id="selectDemoList" resultMap="devMap">
		SELECT * FROM DEV
	</select>
</mapper>
